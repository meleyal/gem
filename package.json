{
  "name": "tuplet",
  "version": "0.0.1",
  "main": "dist/tuplet.js",
  "files": [
    "/dist",
    "/samples"
  ],
  "scripts": {
    "start": "npm run server & npm run samples & npm run website",
    "server": "NODE_ENV=development webpack-dev-server",
    "samples": "http-server --cors --silent",
    "website": "cd website && npm run start",
    "test": "mocha --require esm --require test/test-helper",
    "deploy": "cd website && npm run deploy",
    "build": "webpack -p"
  },
  "repository": "meleyal/tuplet",
  "author": "William Meleyal <william.meleyal@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash-es": "^4.17.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "esm": "^3.2.25",
    "http-server": "^0.12.1",
    "mocha": "^6.2.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
